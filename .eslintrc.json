{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off",
    "prefer-const": "error",
    "no-var": "error",
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "@typescript-eslint/no-unused-vars": "off",
    "no-unused-vars": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}